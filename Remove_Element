class Solution {
public:
    int removeElement(int A[], int n, int elem) {
        // Start typing your C/C++ solution below
        // DO NOT write int main() function
        int i,j=0;
        for (i=0;i<n;i++){
            if (A[i]!=elem){
                A[j] = A[i];
                j++;
            }
        }
        return j;
    }
};