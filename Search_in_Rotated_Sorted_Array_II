class Solution {
public:

    bool search(int A[], int n, int target) {
        // Start typing your C/C++ solution below
        // DO NOT write int main() function

        //因为当中间数和左端数相等时无法二分，所以只好暴力枚举
        for (int i=0;i<n;i++){
            if (A[i]==target) return true;
        }
        return false;
        
    }
};